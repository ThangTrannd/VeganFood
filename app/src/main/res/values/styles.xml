<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SliedAnimation">
        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@android:anim/slide_out_right</item>
    </style>

    <style name="RadioButtonStyle" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/radiobutton</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="android:textSize">@dimen/dimen_16sp</item>
    </style>

    <style name="CheckBoxStyle" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/radiobutton</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="android:textSize">@dimen/dimen_16sp</item>
    </style>

    <style name="TextNormal" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">@dimen/size_20sp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="TextNormal2" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">@dimen/dimen_16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/push_top_down</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_up</item>
    </style>

</resources>